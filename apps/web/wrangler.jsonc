{
  "name": "packbase-web",
  "main": "./dist/_worker.js",
  "compatibility_date": "2025-08-16",
  "assets": {
    "directory": "./dist/",
    "binding": "ASSETS",
    "not_found_handling": "single-page-application",
    "run_worker_first": true
  },
  // Backend API URL for SSR context injection
  // Set via: wrangler secret put VITE_YAPOCK_URL
  // Or in Cloudflare dashboard: Workers > Settings > Variables
  "vars": {
    "VITE_YAPOCK_URL": "https://vgs.packbase.app"
  }
}