<?xml version="1.0" encoding="UTF-8"?>
<loadBalancer>
  <instances min="1" max="10" thresholdMin="0.4" thresholdMax="0.9" cleanupInterval="1" />
  <ports range="8000-8100" proxy="8102" />
  <algorithms primary="round-robin" fallback="least-connections" />
  <security>
    <rateLimit requests="1000" window="60" />
    <cors enabled="true" allowedOrigins="*" allowCredentials="true" allowedMethods="*" allowedHeaders="*" />
    <ssl enabled="false" cert="path/to/cert" />
  </security>
  <clustering>
    <role>parent</role>
    <parentUrl>localhost</parentUrl>
    <syncInterval>30</syncInterval>
  </clustering>
  <chatServer enabled="true">
    <instances min="1" max="5" cleanupInterval="1" />
    <ports range="9000-9100" />
  </chatServer>
</loadBalancer>
